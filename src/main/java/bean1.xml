<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
	<!-- 集合类型属性注入 -->
	<bean id="stu" class="com.ruiheng.spring5.collectiontype.Stu">
		<!-- 数组类型属性注入 -->
		<property name="courses">
			<array>
				<value>Java</value>
				<value>Mysql</value>
			</array>
		</property>
		
		<!-- list -->
		<property name="list">
			<list>
				<value>Ruiheng</value>
				<value>Jinyun</value>		
			</list>
		</property>
		
		<property name="maps">
			<map>
				<entry key="JAVA" value="java"></entry>
				<entry key="PHP" value="php"></entry>
			</map>
		</property>
		
		<property name="sets">
			<set>
				<value>MySQL</value>
				<value>Redis</value>
			</set>
		</property>
		
		
		<property name="courseList">
			<list>
				<ref bean="course1"></ref>
				<ref bean="course2"></ref>
			</list>
		</property>
	</bean>
	
	
	<bean id="course1" class="com.ruiheng.spring5.collectiontype.Course">
		<property name="cname" value="123"></property>
	</bean>
	
	<bean id="course2" class="com.ruiheng.spring5.collectiontype.Course">
		<property name="cname" value="Mybatis"></property>
	</bean>
</beans>